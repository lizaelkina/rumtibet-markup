div.modal.modal__images
  div.modal__container
    button.btn.btn_icon.modal__button(type='button' aria-label='Закрыть окно')
      +svg('close').btn_icon_svg
    div.swiper.modal__swiper
      div.swiper-wrapper
        div.swiper-slide
          +picture('./img/photo/big/photo_1-big', 'Каппадокия').modal__img
          div.swiper-lazy-preloader.swiper-lazy-preloader-white
        div.swiper-slide
          +picture('./img/photo/big/photo_2-big', 'Карта путешественника').modal__img
          div.swiper-lazy-preloader.swiper-lazy-preloader-white
        div.swiper-slide
          +picture('./img/photo/big/photo_3-big', 'Бурдж-эль-Араб').modal__img
          div.swiper-lazy-preloader.swiper-lazy-preloader-white
        div.swiper-slide
          +picture('./img/photo/big/photo_4-big', 'Морская бухта').modal__img
          div.swiper-lazy-preloader.swiper-lazy-preloader-white
        div.swiper-slide
          +picture('./img/photo/big/photo_5-big', 'Великий каньон').modal__img
          div.swiper-lazy-preloader.swiper-lazy-preloader-white
        div.swiper-slide
          +picture('./img/photo/big/photo_6-big', 'Заметки путешественника').modal__img
          div.swiper-lazy-preloader.swiper-lazy-preloader-white
      div.swiper-button-prev.swiper-button-prev_modal
      div.swiper-button-next.swiper-button-prev_modal